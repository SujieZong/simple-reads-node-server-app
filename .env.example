# SimpleReads Environment Configuration
NODE_ENV=development
PORT=4000

# Session secret (use a more secure key in production)
SESSION_SECRET=simplereads_secret_key_change_in_production

# Google Books API
GOOGLE_BOOKS_API_KEY=your_google_books_api_key_here
GOOGLE_BOOKS_API_URL=https://www.googleapis.com/books/v1

# Database configuration (if using MongoDB later)
# MONGODB_URI=mongodb://localhost:27017/simplereads

# CORS configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# File upload configuration
MAX_FILE_SIZE=5MB
UPLOAD_PATH=/uploads
